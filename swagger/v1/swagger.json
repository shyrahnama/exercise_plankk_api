    "/v1/influencers/{influencer_id}/workouts": {
      "get": {
        "summary": "Lists workouts for a given influencer",
        "tags": [
          "Workouts"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
