{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/v1/influencers": {
      "get": {
        "summary": "Lists influencers",
        "tags": [
          "Influencers"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "type": "string",
            "required": true,
            "description": "JWT authorization token. Shayan to provide."
          },
          {
            "name": "name",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list of influencers by name"
          },
          {
            "name": "instagram",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list of influencers by instagram handle"
          },
          {
            "name": "twitter",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list of influencers by twitter handle"
          }
        ],
        "responses": {
          "200": {
            "description": "influencers listed, with filters applied if specified",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer"
                      },
                      "type": {
                        "type": "string"
                      },
                      "attributes": {
                        "type": "object",
                        "properties": {
                          "name": {
                            "type": "string"
                          },
                          "instagram": {
                            "type": "string"
                          },
                          "twitter": {
                            "type": "string"
                          },
                          "signup_date": {
                            "type": "string"
                          },
                          "age": {
                            "type": "integer"
                          },
                          "relationships": {
                            "type": "object",
                            "properties": {
                              "workouts": {
                                "type": "array",
                                "items": {
                                  "type": "object",
                                  "properties": {
                                    "id": {
                                      "type": "integer"
                                    },
                                    "type": {
                                      "type": "string"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Creates an influencer",
        "tags": [
          "Influencers"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "type": "string",
            "required": true,
            "description": "JWT authorization token"
          },
          {
            "name": "influencer",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "instagram": {
                  "type": "string"
                },
                "twitter": {
                  "type": "string"
                },
                "birth_date": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "instagram",
                "twitter",
                "birth_date"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "influencer created"
          },
          "401": {
            "description": "not authorized"
          },
          "422": {
            "description": "invalid request"
          }
        }
      }
    },
