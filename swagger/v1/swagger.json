                    "id": {
                      "type": "integer"
                    },
                    "type": {
                      "type": "string"
                    },
                    "attributes": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "instagram": {
                          "type": "string"
                        },
                        "twitter": {
                          "type": "string"
                        },
                        "signup_date": {
                          "type": "string"
                        },
                        "age": {
                          "type": "integer"
                        },
                        "relationships": {
                          "type": "object",
                          "properties": {
                            "workouts": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "id": {
                                    "type": "integer"
                                  },
                                  "type": {
                                    "type": "string"
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "influencer not found"
          }
        }
      }
