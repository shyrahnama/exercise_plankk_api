            "required": true,
            "description": "JWT authorization token. Shayan to provide."
          },
          {
            "name": "influencer_id",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "name": "title",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list of workouts by title"
          },
          {
            "name": "description",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list of workouts by keywords in description (simple search)"
          },
          {
            "name": "is_private",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "Filter list by whether workout is public or private. Allowable values are 'any', 'true' or 'false'"
          }
        ],
        "responses": {
          "200": {
            "description": "workouts for influencer are returned",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer"
                      },
                      "type": {
                        "type": "string"
                      },
                      "attributes": {
                        "type": "object",
                        "properties": {
                          "title": {
                            "type": "string"
                          },
                          "description": {
                            "type": "string"
                          },
                          "duration_mins": {
                            "type": "integer"
                          },
